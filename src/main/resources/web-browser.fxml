                        <MenuItem mnemonicParsing="false" onAction="#addFavoritesSite" text="addFavoritesSite" />
                        <MenuItem mnemonicParsing="false" onAction="#showFavorites" text="showFavorites" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#editHTML" text="Edit html" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
