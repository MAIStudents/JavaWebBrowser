        <Button layoutX="40.0" layoutY="42.0" mnemonicParsing="false" onAction="#forward" text="&gt;" />
        <TabPane fx:id="tabPane" layoutX="2.0" layoutY="102.0" prefHeight="511.0" prefWidth="728.0" tabClosingPolicy="UNAVAILABLE">
