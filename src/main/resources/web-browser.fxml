        <Button layoutX="4.0" layoutY="75.0" mnemonicParsing="false" onAction="#loadPage" text="load" />
        <Button layoutX="43.0" layoutY="75.0" mnemonicParsing="false" onAction="#refreshPage" text="refresh" />
        <Button layoutX="96.0" layoutY="75.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
        <Button layoutX="121.0" layoutY="75.0" mnemonicParsing="false" onAction="#zoomOut" text="-" />
        <Button layoutX="142.0" layoutY="75.0" mnemonicParsing="false" onAction="#displayHistory" text="History" />
