            </tabs>
        </TabPane>
        <Button layoutX="196.0" layoutY="75.0" mnemonicParsing="false" onAction="#createTab" text="addTab" />
        <Button layoutX="252.0" layoutY="75.0" mnemonicParsing="false" onAction="#saveZip" text="SaveZip" />
        <MenuBar layoutX="6.0" layoutY="3.0" prefHeight="26.0" prefWidth="155.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#createHTML" text="Create html" />
